<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootswatch/3.3.6/flatly/bootstrap.min.css" rel="stylesheet" integrity="sha384-XYCjB+hFAjSbgf9yKUgbysEjaVLOXhCgATTEBpCqT1R3jvG5LGRAK5ZIyRbH5vpX" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/admin.css">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
</head>
<body>

{% include "fragments/admin/navigation" %}

<section>
    <div class="container">
        <div class="row ">
            <div class="col-md-3">
                {% include 'fragments/admin/side-nav' %}
            </div>
            <div class="col-md-8">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>
</section>

{% include "fragments/admin/footer" %}

<script src="/js/jquery-2.2.3.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
</body>
</html>